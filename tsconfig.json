{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["dom","dom.iterable","esnext"],
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": ["**/*.ts","**/*.tsx"],
  "exclude": ["node_modules"]
}